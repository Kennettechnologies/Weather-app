{
    "version": 2,
    "builds": [
        {
            "src": "weather_project/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "weather_project/wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DJANGO_SETTINGS_MODULE": "weather_project.settings"
    }
} 